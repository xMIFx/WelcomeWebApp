<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:gwt='urn:import:com.google.gwt.user.client.ui'
             ui:baseMessagesInterface="com.gitHub.xMIFx.welcomeWebApp.client.i18n.LoginUiMessages">
    <ui:with type="com.gitHub.xMIFx.welcomeWebApp.client.login.LoginResources" field="res">
    </ui:with>
    <gwt:HTMLPanel styleName="{res.style.background}">
        <gwt:FlowPanel>
            <gwt:Label styleName="{res.style.blackText}" addStyleNames="{res.style.inlineBlock}">
                <ui:msg key="login">Not translated</ui:msg>
            </gwt:Label>
            <gwt:TextBox ui:field="loginBox" styleName="{res.style.box}" addStyleNames="{res.style.inlineBlock}"/>
        </gwt:FlowPanel>
        <gwt:FlowPanel>
            <gwt:Label styleName="{res.style.blackText}" addStyleNames="{res.style.inlineBlock}">
                <ui:msg key="password">Not translated</ui:msg>
            </gwt:Label>
            <gwt:PasswordTextBox ui:field="passwordBox"
                                 styleName="{res.style.box}" addStyleNames="{res.style.inlineBlock}"/>
        </gwt:FlowPanel>
        <gwt:Button ui:field="buttonSubmit"  styleName="{res.style.loginButton}">
            <ui:msg key="submit">Not translated</ui:msg>
        </gwt:Button>

    </gwt:HTMLPanel>
</ui:UiBinder>