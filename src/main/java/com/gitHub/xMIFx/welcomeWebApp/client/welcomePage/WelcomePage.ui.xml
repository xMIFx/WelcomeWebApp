<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:gwt='urn:import:com.google.gwt.user.client.ui'
             ui:baseMessagesInterface="com.gitHub.xMIFx.welcomeWebApp.client.i18n.LoginUiMessages">
    <ui:with type="com.gitHub.xMIFx.welcomeWebApp.client.login.LoginResources" field="res">
    </ui:with>
    <gwt:HTMLPanel styleName="{res.style.background}">
        <gwt:Label ui:field="welcome"  styleName="{res.style.blackText}"/>
    <gwt:Button ui:field="buttonExit"  styleName="{res.style.loginButton}">
        <ui:msg key="exit">Not translated</ui:msg>
    </gwt:Button>
    </gwt:HTMLPanel>
</ui:UiBinder>