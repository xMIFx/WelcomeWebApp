<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:gwt='urn:import:com.google.gwt.user.client.ui'
             ui:baseMessagesInterface="com.gitHub.xMIFx.welcomeWebApp.client.i18n.LoginUiMessages">
    <ui:with type="com.gitHub.xMIFx.welcomeWebApp.client.login.LoginResources" field="res">
    </ui:with>
    <gwt:HTMLPanel addStyleNames="{res.style.welcomeBackground}">
        <gwt:FlowPanel>
            <gwt:Label ui:field="welcome"/>
            <gwt:Label ui:field="buttonExit" styleName="{res.style.exitButton}">
                <ui:msg key="exit">Not translated</ui:msg>
            </gwt:Label>
        </gwt:FlowPanel>
    </gwt:HTMLPanel>
</ui:UiBinder>